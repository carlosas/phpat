{
  "name": "carlosas/phpat",
  "type": "library",
  "description": "PHP Architecture Tester",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos Alandete Sastre",
      "email": "carlos.alandete@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "nikic/php-parser": "4.2.*",
    "symfony/dependency-injection": "4.2.*",
    "symfony/event-dispatcher": "4.2.*",
    "symfony/finder": "4.2.*",
    "symfony/yaml": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PhpAT\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\PhpAT\\": "tests/"
    }
  },
  "bin": [
    "phpat"
  ]
}
